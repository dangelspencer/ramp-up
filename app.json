{
  "expo": {
    "name": "RampUp",
    "slug": "rampup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "rampup",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f97316"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rampup.app",
      "infoPlist": {
        "NSHealthShareUsageDescription": "RampUp needs access to read your height from Apple Health.",
        "NSHealthUpdateUsageDescription": "RampUp needs access to write your weight, body fat, and lean mass measurements to Apple Health."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f97316"
      },
      "package": "com.rampup.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#f97316"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
