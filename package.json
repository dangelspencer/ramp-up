    "@react-navigation/native-stack": "^7.9.0",
    "drizzle-orm": "^0.45.1",
    "expo": "~54.0.31",
    "expo-audio": "~1.1.1",
    "expo-crypto": "^15.0.8",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
