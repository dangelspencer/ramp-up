    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
